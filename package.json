{
  "name": "rollup-demo",
  "version": "1.0.0",
  "description": "rollup learning",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rollup --config",
    "build:debug": "rollup --config --configDebug",
    "build:iife": "rollup src/main.js --file dist/bundle-iife.js --format iife",
    "build:cjs": "rollup src/main.js --file dist/bundle-cjs.js --format cjs",
    "build:umd": "rollup src/main.js --file dist/bundle-umd.js --format umd --name 'myBundle'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Awu1227/rollup-demo.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Awu1227/rollup-demo/issues"
  },
  "homepage": "https://github.com/Awu1227/rollup-demo#readme",
  "devDependencies": {
    "rollup": "^4.0.2"
  }
}